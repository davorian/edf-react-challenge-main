{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    "babel-plugin-transform-typescript-metadata"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-refresh/babel"
      ]
    },
    "production": {
      "plugins": [
        "babel-plugin-transform-typescript-metadata",
        "@babel/plugin-transform-react-constant-elements"
      ]
    }
  }
}